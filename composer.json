{
    "name": "Xalior/profanity-bundle",
    "description": "Checker for bad words.",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Ivan Griga",
            "email": "grigaivan2@gmail.com"
        },
        {
            "name": "D. 'Xalior' Rimron-Soutter",
            "email": "darran@xalior.com"
        }
    ],
    "keywords": ["symfony", "bad", "words", "bundle", "profanity", "filter", "censor", "swearword", "swear"],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.1|^8.0",
        "symfony/framework-bundle": "^3.4|^4.0|^5.0|^6.0|^7.0",
        "symfony/validator": "^3.4|^4.0|^5.0|^6.0|^7.0",
        "symfony/yaml": "^3.4|^4.0|^5.0|^6.0|^7.0",
        "symfony/console": "~3.4|~4.0|^5.0|^6.0|^7.0",
        "doctrine/orm": ">=2.4"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "For integration with Doctrine",
        "doctrine/orm": "For integration with Doctrine ORM"
    },
    "autoload": {
        "psr-4": {"Vangrg\\ProfanityBundle\\": ""}
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Xalior/ProfanityBundle"
        }
    ],
    "version": "1.0.0" 
}
